            "precalc": {
                "flagCodeComputed": true,
                "code": "<[1]<sectionName=\"<Top Level>\">\n<_aliasesAtThisLevel=<<%str%\ndbo_Company\n%str%>\n<maxRowsToDisplay=1>\n<cascadingDropdownsInSection=\"\">\n<alias<[1]<aliasName=\"dbo_Company\">\n<tableInfoForAlias=<<%str%\nID=N|T|T|Integer32|!!|ID\nName=C|F|T|VaryingCharacter|!!|Name\nWKPhone=C|F|T|VaryingCharacter|!!|WKPhone\nWKPhoneExt=C|F|T|VaryingCharacter|!!|WKPhoneExt\nDBA=C|F|T|VaryingCharacter|!!|DBA\nStateIncorporated=C|F|T|VaryingCharacter|!!|StateIncorporated\nFEIN=C|F|T|VaryingCharacter|!!|FEIN\nSUIN=C|F|T|VaryingCharacter|!!|SUIN\nSUIRate=N|F|T|Number|!!|SUIRate\nFax=C|F|T|VaryingCharacter|!!|Fax\nStatus=C|F|T|VaryingCharacter|!!|Status\nDescription=C|F|F|VaryingCharacter|!!|Description\nHistory=C|F|F|VaryingCharacter|!!|History\nLongitude=N|F|T|Number|!!|Longitude\nLatitude=N|F|T|Number|!!|Latitude\nDateCreated=T|F|T|DateTime|!!|DateCreated\nCreatedBy=C|F|T|VaryingCharacter|!!|CreatedBy\nDateModified=T|F|T|DateTime|!!|DateModified\nAddress1=C|F|T|VaryingCharacter|!!|Address1\nAddress2=C|F|T|VaryingCharacter|!!|Address2\nAddress3=C|F|T|VaryingCharacter|!!|Address3\nCity=C|F|T|VaryingCharacter|!!|City\nState=C|F|T|VaryingCharacter|!!|State\nZipcode=C|F|T|VaryingCharacter|!!|Zipcode\nAddressBlock=C|F|F|VaryingCharacter|!!|AddressBlock\nStateID=C|F|T|VaryingCharacter|!!|StateID\nActive=L|F|T|Boolean|!!|Active\nHRMgrID=C|F|T|VaryingCharacter|!!|HRMgrID\nIndustryID=N|F|T|Integer32|!!|IndustryID\nLogo=B|F|F|VaryingBinary|!!|Logo\nDefaultProbation=N|F|T|Integer32|!!|DefaultProbation\nModifedBy=C|F|T|VaryingCharacter|!!|ModifedBy\nLastAlertWeek=N|F|T|Integer8|!!|LastAlertWeek\nAutoSendBirthday=L|F|T|Boolean|!!|AutoSendBirthday\nEmailTemplateID=N|F|T|Integer32|!!|EmailTemplateID\nCACompanyNo=C|F|T|VaryingCharacter|!!|CACompanyNo\nWSIBAccountNo=C|F|T|VaryingCharacter|!!|WSIBAccountNo\nONHealthTaxRegNo=C|F|T|VaryingCharacter|!!|ONHealthTaxRegNo\nROEWebAccessNo=C|F|T|VaryingCharacter|!!|ROEWebAccessNo\nCountry=C|F|T|VaryingCharacter|!!|Country\nCanCopy